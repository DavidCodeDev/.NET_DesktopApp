<Window x:Class="ControlasReal.DeleteCardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlasReal"
        mc:Ignorable="d"
        Title="DeleteCardWindow" Height="200" Width="400">
    <Canvas>
        <ComboBox Height="22" Name="cbUsuarios" Canvas.Left="150" Canvas.Top="50" Width="200" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Label Content="Selecciona usuario:" Height="24" Canvas.Left="39" Canvas.Top="50" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        <Button Content="Desvincular tarjeta" Click="EliminarTarjeta_Click" Canvas.Left="150" Canvas.Top="100" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Canvas>
</Window>
